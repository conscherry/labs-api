{
  "name": "@conscherry/labs-api",
  "version": "1.0.1",
  "description": "A Node.js/TypeScript client for the Conscherry Labs API (list bots, post stats, read users and website stats).",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.{ts,js}\" README.md package.json",
    "lint": "echo \"No lint configured\"",
    "test": "jest --passWithNoTests",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conscherry/labs-api.git"
  },
  "keywords": [
    "labs",
    "conscherry",
    "discord",
    "api",
    "nodejs",
    "library",
    "bots",
    "stats",
    "wrapper",
    "typescript"
  ],
  "author": "Xavier Garcia <xavier.garcia@conscherry.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/conscherry/labs-api/issues"
  },
  "homepage": "https://github.com/conscherry/labs-api#readme",
  "files": [
    "dist",
    "src",
    "README.md",
    ".prettierrc",
    ".env.example"
  ],
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "dist",
    "src": "src"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.4.6",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "node-fetch": "^3.3.2",
    "undici": "^7.18.1"
  }
}
